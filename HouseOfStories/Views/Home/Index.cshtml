
<div class="fixed"   style=" height: auto ; width: 90% !important;" >


    <br />
    <div class="column">
        @for (var i = 0; i < 100; i++)
        {
            <partial name="_IndividualQuote" />
        }

    </div>
</div>

@section Scripts{
<script>
        $(document).ready(function(){

        $(".filter-button").click(function(){

            var value = $(this).attr('data-filter');
            if(value == "all")
            {
                $('.filter').show('1000');
            }
            else
            {
                $(".filter").not('.'+value).hide('3000');
                $('.filter').filter('.'+value).show('3000');

            }
        });

    });
</script>
}
